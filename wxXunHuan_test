package wxXunHuan

import (
	"fmt"
	"testing"
)

func Test0001(test *testing.T) {
	list := CreateList()
	s := []string{"子", "丑","寅","卯","辰","巳","午","未","申","酉","戌","亥"}
	for _, v := range s {
		list.PushBack(v)
	}
	list.Print()
	fmt.Println(list.FindByAngle(-16).Data)
	list.Print()
	list.ForWard(2)
	list.Print()
	list.Reverse()
	list.Print()
}
