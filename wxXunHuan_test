package wxXunHuan

import (
	"fmt"
	"testing"
)

func Test0001(test *testing.T) {
	list := CreateList()
	//s := []string{"子", "丑","寅","卯","辰","巳","午","未","申","酉","戌","亥"}
	s := []string{"子", "癸","丑","艮","寅","甲","卯","乙","辰","巽","巳","丙","午","丁","未","坤","申","庚","酉","辛","戌","乾","亥","壬"}
	for _, v := range s {
		list.PushBack(v)
	}
	list.Print()
	fmt.Println(list.FindByAngle(-16).Data)
	list.Print()
	list.ForWard(2)
	list.Print()
	list.Reverse()
	list.Print()
}
