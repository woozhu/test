package wxXunHuan

import (
	"testing"
)

func Test0001(test *testing.T) {
	list := CreateList()
	s := []string{"木", "火", "土", "金", "水"}
	for _, v := range s {
		list.PushBack(v)
	}
	list.Print()
	list.BackWard(3)
	list.Print()
	list.ForWard(3)
	list.Print()
	list.ForWard(2)
	list.Print()
	list.Reverse()
	list.Print()

}

